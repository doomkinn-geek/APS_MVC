{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SmtpConfig": {
    "Host": "smtp.beget.com",
    "Port": "25",
    "EnableLogging": "false",
    "FromAddress": "doomkinn@test.ru"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [      
        {
          "Name": "Console"
        },
        {
          "Name": "File",
          "Args": { "path": "Logs/logs.log" }
        }
    ]
  }
}